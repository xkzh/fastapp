{
  "package": "com.xk.fastapp",
  "name": "fastapp",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "minPlatformVersion": 1000,
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.clipboard"
    },
    {
      "name": "system.share"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "Main",
    "pages": {
      "Hello": {
        "component": "hello",
        "path": "/"
      },
      "Main": {
        "component": "main",
        "path": "/main"
      }
    }
  },
  "display": {
    "backgroundColor": "#fbf9fe",
    "titleBar": true,
    "titleBarBackgroundColor": "#0faeff",
    "titleBarTextColor": "#ffffff",
    "menu": true,
    "pages": {
      "Hello": {
        "titleBarText": "TitleBar"
      }
    }
  },
  "debugable": true
}